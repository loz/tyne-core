<%= cache ["v1", "project", "dialog"] do %>
  <div id="project_dialog" class="dialog" title="Create project">
    <%= simple_form_for(project, :validate => true, :html => { :data => { :resource_url => url_for(:projects) }, :method => "put" }) do |f| %>
      <%= f.input :key %>
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.button :submit, :class => "btn btn-primary" %>
    <% end %>
  </div>
<% end %>
